<?xml version="1.0"?>
<environments>
	<!-- 
		'name': A descriptive name (has no purpose outside of UI)
		'protected': If present, and set to "true", this environment is considered a production environment
		'service': Name of the Azure cloud service containing the config data
		'subscription': Name of the Azure subscription containing the service -->
	<environment name="Production" protected="true" service="mynugetgallery" subscription="My Subscription" />
</environments>
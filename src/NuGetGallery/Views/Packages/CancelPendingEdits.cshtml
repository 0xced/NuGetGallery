@model IPackageVersionModel
@{
    ViewBag.Tab = "Packages";
}

<h1 class="page-heading">Cancel Edits for @Model.Id @Model.Version</h1>
<p class="message">
    Click cancel to revert ALL pending metadata edits for package <b>@Model.Id @Model.Version</b>
</p>
<p>
    Note: pending edits might complete and thereby become non-pending any minute, so please decide quickly.
</p>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <input type="hidden" name="dummy" value="true" />
    <input type="submit" value="Cancel" title="Cancel Pending Edits" />
}